{
  "name": "chatroom-cli",
  "version": "1.0.7",
  "description": "CLI for multi-agent chatroom collaboration",
  "type": "module",
  "bin": {
    "chatroom": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "chatroom": "bun run src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "convex": "^1.31.0",
    "node-notifier": "^10.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/node-notifier": "^8.0.5",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "cli",
    "chatroom",
    "multi-agent",
    "convex"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
